{
  "data_source": {
    "root_path": "./input",
    "path_fmt": "",
    "fn_pattern": "%Y%m%d%H%M_radar.rack.comp_CONF=FMIPPN,ANDRE",
    "fn_ext": "h5",
    "importer": "opera_hdf5",
    "timestep": 5,
    "importer_kwargs": {"gzipped": false, "qty": "DBZH"}
  },

  "data_options": {},

  "motion_options": {},

  "nowcast_options": {
    "timesteps": 12,
    "n_ens_members": 5,
    "n_cascade_levels": 8,
    "timestep": 5,
    "domain": "spectral",
    "kmperpixel": 1.0,
    "num_workers": 10,
    "fft_method": "pyfftw",
    "vel_pert_method": "bps",
    "vel_pert_kwargs": {
      "p_par": [
        2.20837526,
        0.33887032,
        -2.48995355
      ],
      "p_perp": [
        2.21722634,
        0.32359621,
        -2.57402761
      ]
    },
    "seed": 1,
    "extrap_method": "semilagrangian",
    "noise_method": "nonparametric",
    "ar_order": 2,
    "mask_method": "incremental"
  },

  "run_options": {},

  "output_options": {},

  "logging": {
    "write_log": true,
    "log_level": 10,
    "log_folder": "./output/logs",
    "log_fname_template": "ppn-%Y%m%d.log"
  },

  "DOMAIN": "ravake_hdf5",
  "OPTFLOW_METHOD": "lucaskanade",

  "GENERATE_DETERMINISTIC": true,
  "GENERATE_ENSEMBLE": true,
  "GENERATE_UNPERTURBED": false,
  "REGENERATE_PERTURBED_MOTION": false,
  "VALUE_DOMAIN": "dbz",
  "ZR_A": 223.0,
  "ZR_B": 1.53,
  "NUM_PREV_OBSERVATIONS": 3,

  "MAX_LEADTIME": 60,
  "NUM_TIMESTEPS": null,

  "RAIN_THRESHOLD": -10,
  "NORAIN_VALUE": -15,

  "FIELD_VALUES": "dbz",
  "OUTPUT_PATH": "./output",
  "OUTPUT_TIME_FORMAT": "%Y-%m-%d %H:%M:%S",
  "STORE_ENSEMBLE": true,
  "STORE_UNPERTURBED": true,
  "STORE_DETERMINISTIC": true,
  "STORE_MOTION": true,
  "STORE_PERTURBED_MOTION": true,
  "SCALER": 10,
  "SCALE_ZERO": -32,
  "WRITE_LOG": true,
  "LOG_LEVEL": 10,
  "LOG_FOLDER": "./output/logs"
}
